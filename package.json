{
  "name": "mentorae",
  "version": "1.0.0",
  "main": "dist-electron/main/index.js",
  "description": "An Academic Tutoring and Peer Support Hub",
  "author": {
    "name": "lGb18",
    "email": "levibarroga18@gmail.com"
  },
  "license": "MIT",
  "private": true,
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:renderer": "vite build",
    "build:main": "vite build --mode=production",
    "build": "tsc && npm run build:main && npm run build:renderer",
    "build:electron": "npm run build && electron-builder --publish never",
    "preview": "vite preview",
    "test": "vitest run",
    "dist": "npm run build:electron-builder"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/supabase-js": "^2.57.4",
    "@types/deno": "^2.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-updater": "^6.3.9",
    "lucide-react": "^0.544.0",
    "next-themes": "^0.4.6",
    "papaparse": "^5.5.3",
    "peerjs": "^1.5.5",
    "react-katex": "^3.1.0",
    "react-quill": "^2.0.0",
    "react-router-dom": "^7.9.1",
    "sonner": "^2.0.7",
    "survey-core": "^2.5.3",
    "survey-react": "^1.12.58",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.2",
    "@types/node": "^24.4.0",
    "@types/papaparse": "^5.5.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-katex": "^3.0.4",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "builder": "^5.0.0",
    "electron": "^38.2.0",
    "electron-builder": "^24.13.3",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "supabase": "^2.70.5",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.4.2",
    "vite": "^7.1.5",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vitest": "^3.2.4"
  },
  "build": {
    "publish": null,
    "appId": "com.mentorae.tutorapp",
    "productName": "Mentorae",
    "files": [
      "dist/**",
      "dist-electron/**",
      "!**/*.map",
      "!**/*.ts",
      "!**/tsconfig.json",
      "!dist/win-unpacked/",
      "package.json"
    ],
    "extraMetadata": {
      "compressed": true
    },
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "arch": [
        "x64"
      ],
      "minimumSystemVersion": "10.13.6",
      "forceCodeSigning": false,
      "category": "public.app-category.education"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Education"
    },
    "win": {
      "forceCodeSigning": false,
      "certificateFile": null,
      "target": [
        "nsis",
        "zip"
      ]
    }
  }
}